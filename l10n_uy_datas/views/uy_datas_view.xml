<odoo>
  <data>
    <record model="ir.ui.view" id="view_uy_datas_tree">
      <field name="name">uy.datas.tree</field>
      <field name="model">uy.datas</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="code"/>
          <field name="data_code"/>
          <field name="active"/>
        </tree>
      </field>
    </record>

	<record id="view_uy_datas_from" model="ir.ui.view">
        <field name="name">uy.datas.form</field>
        <field name="model">uy.datas</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <group>
                        <field name="name"/>
                        <field name="code"/>
                    </group>
                    <group>
                        <field name="data_code"/>
                        <field name="active"/>
                    </group>
                </group>
                <label for="description"/>
                <field name="description" />
            </form>
        </field>
    </record>
    
